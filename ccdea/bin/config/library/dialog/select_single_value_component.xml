<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<config version='1.0'>

    <scope>

        <component id="select_single_value_component">

            <desc>
                Компонент для выбора значения из отфильтрованного списка
            </desc>

            <params>
                <param name="valueType" required="true"/>
                <param name="valueStart" required="false"/>
            </params>

            <pages>
                <start>/ccdea/library/dialog/select_single_value_dialog.jsp</start>
            </pages>

            <class>ru.rb.ccdea.dialog.SelectSingleValueComponent</class>
            <nlsbundle>ru.rb.ccdea.dialog.SelectSingleValueNlsProp</nlsbundle>
        </component>

    </scope>
</config>

